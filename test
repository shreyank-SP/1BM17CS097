class matches:
    dict1={}
    def __init__(self,team,cn,mp,mw):
    
        self.team=team
        self.cn=cn
        self.mp=mp
        self.mw=mw

    
    def make_dict(self):
        
        self.dict1[self.team]=[self.cn,self.mp,self.mw]
    

    def find_matches(self,country_name):
        a=self.dict1[country_name]
        print(country_name +" has played in "+a[0] )
        print(country_name +" has played " +a[1])
        print(country_name +" has won " +a[2])   
    
    def find_winner(self,country1,country2):
    
        lista=self.dict1[country1]
        listb=self.dict1[country2]

        a=lista[2]
        b=listb[2]

        if(a==b):
           print("Tie")
        elif (a>b):
             print(country1 +" is the winner")
        else:
            print(country2 +" is the winner")
    


class util():

    def __init__(self):
   
       self.list_of_MD=[]
   
    def allot(self,listtMD):
      
        for i in listMD:
        
            a1=i.split(":")
            o=matches(a1[0],a1[1],a1[2],a1[3])
            o.make_dict()
        
    
match_detail1="AUS:CHAM:5:2"
match_detail2="IND:CHAM:5:4"
match_detail3="SA:CHAM:5:3"

listMD=[match_detail1,match_detail2,match_detail3]
ob=util()
ob.allot(listMD)
ob1=matches(0,0,0,0)
ob1.find_matches("IND")
ob1.find_winner("IND","AUS")



          
